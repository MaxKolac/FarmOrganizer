<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    
    <Color x:Key="LightPrimary">#4E9231</Color>
    <Color x:Key="LightSecondary">#A8DB92</Color>
    <Color x:Key="LightTetriary">#2E6E12</Color>
    
    <Color x:Key="DarkPrimary">#4E9231</Color>
    <Color x:Key="DarkSecondary">#A8DB92</Color>
    <Color x:Key="DarkTetriary">#2E6E12</Color>

    <Style TargetType="Button">
        <Setter Property="BackgroundColor" 
                    Value="{AppThemeBinding Light={StaticResource LightPrimary}, Dark={StaticResource DarkTetriary}}"/>
    </Style>
</ResourceDictionary>